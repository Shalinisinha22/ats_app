{
  "expo": {
    "name": "EmpQuick",
    "slug": "empquick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1dbf73"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS"
      ],
      "package": "com.raja7252725272528752.apps",
      "runtimeVersion": "1.0.0",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "notification": {
        "icon": "./assets/icon.png",
        "color": "#ffffff",
        "androidMode": "default",
        "androidCollapsedTitle": "New Notification",
        "iosDisplayInForeground": true,
        "androidImportance": "high",
        "androidPriority": "high",
        "androidShowBadge": true,
        "androidVibrate": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "shalinisinha21",
    "extra": {
      "eas": {
        "projectId": "6c97dc2e-6b30-462f-b7ea-58c73598fceb"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/ce9baffd-b755-4d11-ab2f-fe6ab81caafe"
    },
    "plugins": [
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "mode": "production",
          "androidMode": "default",
          "androidCollapsedTitle": "New Notification",
          "androidImportance": "high",
          "androidColor": "#1dbf73",
          "androidChannelId": "default",
          "androidShowWhen": true,
          "androidPriority": "high",
          "androidVibrate": [0, 250, 250, 250],
          "androidSticky": false
        }
      ]
    ]
  }
}
